{
  "name": "api",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "meili": "powershell -NoProfile -ExecutionPolicy Bypass -File ..\\..\\tools\\meili\\start-meili.ps1 -DbPath \".\\data.ms\"",
    "reindex:laws": "node --import tsx scripts/index_laws_meili.ts",
    "reindex:segments": "node --import tsx scripts/index_segments_meili.ts",
    "reindex": "npm run reindex:laws && npm run reindex:segments",
    "setup:meili": "npm run meili && npm run reindex",
    "setup:meili:dev": "npm run meili && npm run reindex && npm run dev",
    "scrape:fbih": "node --import tsx scripts/scrape_fbih_laws.ts",
    "segments:fbih": "JURISDICTION=FBiH LIMIT=50 node --import tsx scripts/extract_segments_rs.ts",
    "check:rs156": "node --import tsx scripts/check_rs_156.ts",
    "insert:rs156": "node --import tsx scripts/insert_rs_156_from_final.ts",
    "compare:erp": "node --import tsx scripts/scrape_rs_erp_headless.ts",
    "compare:sgtxt": "node --import tsx scripts/compare_sg_txt_vs_db.ts",
    "compare:sgtxt-vs-nsrs": "node --import tsx scripts/compare_sgtxt_missing_vs_nsrs.ts",
    "compare:sgtxt-vs-nsrs-preuzmi": "node --import tsx scripts/count_preuzmi_for_sgtxt_vs_nsrs.ts",
    "import:brcko": "node --import tsx scripts/import_brcko_from_xlsx.ts",
    "import:montenegro": "node --import tsx scripts/import_montenegro_from_xlsx.ts",
    "segments:montenegro": "JURISDICTION='Crna Gora' LIMIT=50 node --import tsx scripts/extract_segments_rs.ts",
    "build": "tsc -p tsconfig.json",
    "start": "node dist/server.js"
  },
  "dependencies": {
    "adm-zip": "^0.5.16",
    "cheerio": "^1.1.2",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^4.19.2",
    "fs-extra": "^11.2.0",
    "iconv-lite": "^0.6.3",
    "libreoffice-convert": "^1.7.0",
    "mammoth": "^1.8.0",
    "meilisearch": "^0.36.0",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "pdf-parse": "^2.4.5",
    "pdfjs-dist": "^5.4.394",
    "puppeteer": "^22.15.0",
    "sqlite3": "^5.1.7",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/multer": "^1.4.11",
    "@types/node": "^22.7.5",
    "ts-node-dev": "^2.0.0",
    "tsx": "^4.20.6",
    "typescript": "^5.6.3"
  }
}
